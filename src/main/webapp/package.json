{
  "name": "arthas-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "iview": "3.1.5",
    "jquery": "3.3.1",
    "vue": "2.5.20",
    "xterm": "3.9.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.2.0",
    "@vue/cli-plugin-eslint": "3.2.1",
    "@vue/cli-service": "3.2.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.10.0",
    "eslint-plugin-vue": "5.0.0",
    "vue-template-compiler": "2.5.20"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-mixed-spaces-and-tabs": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
